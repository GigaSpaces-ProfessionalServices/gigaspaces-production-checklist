ac_default_prefix=/usr/local
ac_clean_files=
ac_config_libobj_dir=.
LIBOBJS=
cross_compiling=no
subdirs=
MFLAGS=
MAKEFLAGS=

# Identity of this package.
PACKAGE_NAME='gigapro-xap'
PACKAGE_TARNAME='gigapro-xap'
PACKAGE_VERSION='version-0.1'
PACKAGE_STRING='gigapro-xap version-0.1'
PACKAGE_BUGREPORT=''
PACKAGE_URL=''

ac_subst_vars='JAVA_PATH_NAME
UUDECODE
JAVA
target_alias
